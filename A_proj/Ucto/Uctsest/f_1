{                                                                
  { "UCETKUM"  1   "TMPKUM"                                                     
    { "nKcMDpsO"   , "nKcDALpsO" , "nKcMDobrO" , "nKcDALobrO" , "nKcMDpsR"   , "nKcDALpsR" , "nKcMDobrR" , "nKcDALobrR" , "nKcMDksR"                                                 
      "nKcDALksR"  , "nMnozNAT"  , "nMnozNAT2" , "nMnozNATR"  , "nMnozNAT2R"     }                                                            
    "TMPKUM ->cUCETMD"                                           
    "UCTKw_01"                                                   
  }                                                              
  { "UCETKUMk"  1   "TMPKUMm"                                                    
    { "nKcMDpsO"  , "nKcDALpsO"  , "nKcMDobrO" , "nKcDALobrO" , "nKcMDpsR"   , "nKcDALpsR"  , "nKcMDobrR"  , "nKcDALobrR" , "nKcMDksR"                                                 
      "nKcDALksR" , "nMnozNAT"   , "nMnozNAT2" , "nMnozNATR"  , "nMnozNAT2R"    }                                                            
    "TMPKUMm ->cUCETMD"                                          
    "UCTKKw_01"                                                  
  }                                                              
  { "UCETPLAN"  1  "TMPPLAN"                                                    
    { "nPLANzaOBD" ,  "nPLANkOBD" , "nPLANROK" , "nPLANzaoOR" , "nMnozNAT"  ,  "nMnozNAT2"    }                                                            
    "TMPPLAN ->cUCETMD"                                          
    "UCTPw_01"                                                   
