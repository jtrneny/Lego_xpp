   var cpexCAFSettings = cpexCAFSettings || {};
   cpexCAFSettings['slideup'] = cpexCAFSettings['slideup'] || {};
   cpexCAFSettings['slideup']['CLOSE']= cpexCAFSettings['slideup']['CLOSE'] || {};
