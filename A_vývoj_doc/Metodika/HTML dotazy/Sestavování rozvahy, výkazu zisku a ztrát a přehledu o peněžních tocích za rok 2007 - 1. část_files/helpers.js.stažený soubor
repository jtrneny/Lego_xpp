'use strict';

var AspiHelpers = function(){
};

AspiHelpers.prototype.getIsoDateFormat = function(date, sep){
  var DateArray = date.split(sep);
  if(DateArray.length == 3){
    return DateArray[2].trim()+"-"+DateArray[1].trim()+"-"+DateArray[0].trim();  
  }else{
    return false;
  }
  
}